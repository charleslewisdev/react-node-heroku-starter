{
  "name": "react-node-heroku-starter",
  "version": "0.0.1",
  "description": "Template for quickly creating Node + React web apps that can be deployed to Heroku in minutes",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "nodemon server",
    "start": "node server",
    "build": "cd client/ && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "prettier": "^2.0.2",
    "sequelize": "^5.21.11",
    "winston": "^3.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charleslewisdev/react-node-heroku-starter.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
